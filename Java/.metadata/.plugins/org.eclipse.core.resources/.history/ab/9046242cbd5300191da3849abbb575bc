package renderer;

import javax.swing.*;

public class Window {
	JFrame mainFrame;
	
	public Window(int width, int height) {
		mainFrame = new JFrame();
		mainFrame.setSize(width, height);
		mainFrame.setLayout(null);
		mainFrame.setVisible(true);
	}
	
	public void run() {
		mainFrame.setVisible(true);
	}
}
